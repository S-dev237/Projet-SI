{
  "version": 2,
  "builds": [
    {
      "src": "messenger/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "messenger/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "messenger.settings"
  }
}
